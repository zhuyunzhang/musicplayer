.playBot{position:fixed;display:-webkit-box;display:-webkit-flex;display:flex;-webkit-box-align:center;-webkit-align-items:center;align-items:center;bottom:0;left:0;width:100vw;padding:20rpx 25rpx 10rpx;box-sizing:border-box;background-color:#fff}.playBot .img{display:block;width:80rpx;height:80rpx;border-radius:50%}.playBot .img.zhuan{-webkit-transform:rotate(1turn);-webkit-animation:rotation 8s linear infinite;animation:rotation 8s linear infinite}.playBot .img.stop{-webkit-transform:rotate(1turn)}.playBot .title{-webkit-box-flex:1;-webkit-flex:1;flex:1;padding-left:30rpx;font-size:34rpx}.playBot .title .desc{font-size:26rpx;color:#4b4b4b}.playBot .icon{width:65rpx;height:65rpx;padding-left:30rpx}.playBot .icon.liebiao{width:40rpx;height:40rpx}