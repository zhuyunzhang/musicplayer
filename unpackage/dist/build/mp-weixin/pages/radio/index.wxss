.playBot{position:fixed;display:-webkit-box;display:-webkit-flex;display:flex;-webkit-box-align:center;-webkit-align-items:center;align-items:center;bottom:0;left:0;width:100vw;padding:20rpx 25rpx 10rpx;box-sizing:border-box}.playBot .img{display:block;width:80rpx;height:80rpx;border-radius:50%}.playBot .img.zhuan{-webkit-transform:rotate(1turn);-webkit-animation:rotation 8s linear infinite;animation:rotation 8s linear infinite}.playBot .img.stop{-webkit-transform:rotate(1turn)}.playBot .title{-webkit-box-flex:1;-webkit-flex:1;flex:1;padding-left:30rpx;font-size:34rpx}.playBot .title .desc{font-size:26rpx;color:#4b4b4b}.playBot .icon{width:65rpx;height:65rpx;padding-left:30rpx}.playBot .icon.liebiao{width:40rpx;height:40rpx}.new{overflow:hidden}.new .bar{display:-webkit-box;display:-webkit-flex;display:flex;-webkit-box-pack:center;-webkit-justify-content:center;justify-content:center;margin:30rpx 0}.new .bar .li{padding:0 40rpx;line-height:60rpx;border:solid 1px #000;color:#000;background:#fff;border-right:none}.new .bar .li:last-child{border-right:solid 1px #000}.new .bar .li.sel{background:#000;color:#fff}.new .swiper{height:calc(100vh - 130rpx)}.new .scroll-Y{height:calc(100vh - 230rpx)}