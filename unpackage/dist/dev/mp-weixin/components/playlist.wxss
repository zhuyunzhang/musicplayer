.playList {
  margin-left: 30rpx;
  padding: 15rpx 25rpx 20rpx 0;
}
.playList .biao {
  display: inline-block;
  vertical-align: text-bottom;
  padding: 2rpx 6rpx;
  margin-right: 10rpx;
  border: solid 1px #f55500;
  color: #f55500;
  border-radius: 4rpx;
  font-size: 24rpx;
  line-height: 24rpx;
}
.playList .biao.sq {
  color: #9c8200;
  border: solid 1px #9c8200;
}
.playList .playListBofang,
.playList .playListGengduo {
  display: block;
  width: 40rpx;
  height: 40rpx;
  margin-left: 45rpx;
  margin-top: 20rpx;
}
.playList .name {
  font-size: 38rpx;
  line-height: 60rpx;
}
.playList .other {
  font-size: 26rpx;
  line-height: 40rpx;
  color: #4b4b4b;
  width: 520rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}

